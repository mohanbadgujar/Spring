package com.bridgelabz;

import com.javatpoint.BeanFactory;
import com.javatpoint.ClassPathResource;
import com.javatpoint.MailMail;
import com.javatpoint.Resource;
import com.javatpoint.XmlBeanFactory;

public class Test {
	public static void main(String[] args) {
		
		Resource r=new ClassPathResource("applicationContext.xml");
		BeanFactory b=new XmlBeanFactory(r);
		MailMail m=(MailMail)b.getBean("mailMail");
		String sender="sonoo8zq@gmail.com";//write here your id
		String receiver="sonoojaiswal1987@gmail.com";//write here receiver id
		m.sendMail(sender,receiver,"hi","welcome");
		
		System.out.println("success");
	}
}
